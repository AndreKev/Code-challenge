    "def ex_stern(N):\n",
    "    n = N//2\n",
    "    stern = [0,1]\n",
    "    k =1\n",
    "    while k<=n:\n",
    "        stern.append(stern[k])\n",
    "        stern.append(stern[k]+stern[k+1])\n",
    "        k += 1\n",
    "    return stern[N]\n",
    "ex_stern(10000001)"
   ]
  }
